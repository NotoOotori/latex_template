{
    "makefile.extensionOutputFolder": "./.vscode",
    "latex-workshop.latex.recipes": [
        {
            "name": "LaTeXmk - XeLaTeX",
            "tools": [
                "LaTeXmk - XeLaTeX",
                "Export Note"
            ]
        },
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "LaTeXmk - XeLaTeX",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "--shell-escape",
                "-xelatex",
                "%DOC%"
            ]
        },
        {
            "name": "Export Note",
            "command": "cp",
            "args": [
                "%DOCFILE%.pdf",
                "/mnt/c/Users/Jason/iCloudDrive/Notes"
            ]
        }
    ],
}
